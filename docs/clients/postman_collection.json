{
  "info": {
    "name": "7rifyConnect API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "description": "Provisional Postman collection inferred from the frontend client."
  },
  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Accept", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"secret\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "login"]
            }
          }
        },
        {
          "name": "Me",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Accept", "value": "application/json" }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/auth/me",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "me"]
            }
          }
        }
      ]
    },
    {
      "name": "Technicians",
      "item": [
        {
          "name": "Search technicians",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Accept", "value": "application/json" }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/technicians?q=plumber",
              "host": ["{{baseUrl}}"],
              "path": ["api", "technicians"],
              "query": [
                { "key": "q", "value": "plumber" }
              ]
            }
          }
        },
        {
          "name": "Technician profile",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Accept", "value": "application/json" }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/technicians/1",
              "host": ["{{baseUrl}}"],
              "path": ["api", "technicians", "1"]
            }
          }
        }
      ]
    },
    {
      "name": "Requests",
      "item": [
        {
          "name": "Create request",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Accept", "value": "application/json" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Fix wiring\",\n  \"city\": \"Casablanca\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/requests",
              "host": ["{{baseUrl}}"],
              "path": ["api", "requests"]
            }
          }
        }
      ]
    }
  ],
  "variable": [
    { "key": "baseUrl", "value": "http://localhost:8000" }
  ]
}
